{
    "name": "theo/crawler.codexdelta",
    "description": "Multipurpose tool for search and find specific patterns from api's or DOM",
    "type": "project",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "phpmailer/phpmailer": "^6.9",
        "symfony/http-foundation": "^7.1",
        "symfony/var-dumper": "^7.1",
        "twig/twig": "^3.0",
        "symfony/dom-crawler": "^7.1",
        "symfony/css-selector": "^7.1",
        "eckinox/php-puppeteer": "^1.0"
    },
    "license": "MIT",
    "autoload": {
        "files": [
            "libs/helpers.php"
        ],
        "psr-4": {
            "Codexdelta\\App\\": "src/",
            "Codexdelta\\Libs\\": "libs/"
        }
    },
    "authors": [
        {
            "name": "Theodoros Dimitriadis",
            "email": "theoddimi@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
